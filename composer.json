{
    "name": "dede/repositories",
    "description": "Simple Repository Implementation (Turorial from bornadev/repositories)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dede Gunawan",
            "email": "dede@unsil.ac.id"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support":"5.*",
        "illuminate/database":"5.*"
    },
    "autoload": {
        "psr-4": {
            "Dede\\Repositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dede\\Tests\\Repositories\\": "tests/"
        }
    },
    "extra": {
        "branch-alias":{
            "dev-master": "0.x-dev"
        }
    },
    "prefer-stability": true

}
